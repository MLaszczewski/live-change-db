    "@live-change/dao-websocket": "^0.3.1",
    "@live-change/vue3-ssr": "^0.1.9",
    "@live-change/vue3-components": "^0.1.11",
    "@live-change/message-authentication-service": "^0.2.4",
    "@live-change/secret-link-service": "^0.2.4",
    "@live-change/secret-code-service": "^0.2.4",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.21",
    "@vueuse/core": "^6.7.4",
    "v-shared-element": "3.1.0",
    "vue3-scroll-border": "^0.1.0",
    "codeceptjs-assert": "^0.0.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "get-port-sync": "1.0.1",
    "primeicons": "^5.0.0",
    "primevue": "^3.8.2",
    "primeflex": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "serialize-javascript": "^6.0.0",
    "serve-static": "^1.14.1",
    "vite": "^2.5.1",
    "vue": "^3.2.21",
    "vue-meta": "3.0.0-alpha.9",
    "vue-router": "^4.0.11",
    "vite-plugin-compression": "0.3.6",
    "vite-plugin-vue-images": "^0.6.1",
    "rollup-plugin-visualizer": "5.5.2",
    "vue-prism-editor": "2.0.0-alpha.2",
    "javascript-stringify": "^2.0.1",
    "prism-es6": "^1.2.0",
    "prismjs": "^1.26.0"
